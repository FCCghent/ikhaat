{
  "name": "ikhaat",
  "version": "1.0.0",
  "description": "Avoid things you don't like in Ghent",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FCCghent/ikhaat.git"
  },
  "scripts": {
    "gipod": "gipod --type workassignment --query city=gent > src/data/werken.geojson",
    "overpass": "node src/overpass",
    "update": "npm run gipod & npm run overpass",
    "serve": "jekyll s --ssl-cert assets/ca.crt --ssl-key assets/ca.key"
  },
  "bugs": {
    "url": "https://github.com/FCCghent/ikhaat/issues"
  },
  "homepage": "https://ikhaat.gent",
  "dependencies": {
    "gipod-to-geojson": "^2.2.0",
    "query-overpass": "^1.1.0"
  },
  "devDependencies": {
    "fs.promised": "^3.0.0",
    "gh-pages-travis": "^1.0.0"
  },
  "license": "MIT"
}
